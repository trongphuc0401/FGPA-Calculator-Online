<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
 <context-param>
  <param-name>fix</param-name>
  <param-value>&lt;multipart-config&gt;
  &lt;location&gt;/tmp&lt;/location&gt;
  &lt;max-file-size&gt;20848820&lt;/max-file-size&gt;
  &lt;max-request-size&gt;418018841&lt;/max-request-size&gt;
  &lt;file-size-threshold&gt;1048576&lt;/file-size-threshold&gt;
&lt;/multipart-config&gt;</param-value>
 </context-param>
 <servlet>
  <servlet-name>LoginServlet</servlet-name>
  <display-name>LoginServlet</display-name>
  <servlet-class>controller.LoginServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>LoginSeervlet</servlet-name>
  <display-name>LoginSeervlet</display-name>
  <servlet-class>controller.LoginSeervlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>DashboardServlet</servlet-name>
  <display-name>DashboardServlet</display-name>
  <servlet-class>controller.DashboardServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>Dashboard</servlet-name>
  <display-name>Dashboard</display-name>
  <servlet-class>controller.Dashboard</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>DashboardSevlet</servlet-name>
  <display-name>DashboardSevlet</display-name>
  <servlet-class>controller.DashboardSevlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>SignUpServlet</servlet-name>
  <display-name>SignUpServlet</display-name>
  <servlet-class>controller.SignUpServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>MenuSevlet</servlet-name>
  <display-name>MenuSevlet</display-name>
  <servlet-class>controller.student.MenuSevlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>LoginServlet</servlet-name>
  <url-pattern>/LoginServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>LoginSeervlet</servlet-name>
  <url-pattern>/LoginSeervlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>DashboardServlet</servlet-name>
  <url-pattern>/DashboardServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Dashboard</servlet-name>
  <url-pattern>/Dashboard</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>DashboardSevlet</servlet-name>
  <url-pattern>/DashboardSevlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>SignUpServlet</servlet-name>
  <url-pattern>/SignUpServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>MenuSevlet</servlet-name>
  <url-pattern>/MenuSevlet</url-pattern>
 </servlet-mapping>
</web-app>
